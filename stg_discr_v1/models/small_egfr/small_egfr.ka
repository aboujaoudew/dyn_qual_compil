####### SMALL EGFR MODEL ########

'r1' EGF(r!1),EGFR(l!1) -> EGF(r),EGFR(l)

'r2' EGF(r),EGFR(l) -> EGF(r!1),EGFR(l!1)

'r3' EGFR(a,l!_),Sos(x) -> EGFR(a!1,l!_),Sos(x!1)

'r4' EGFR(a!_) -> EGFR(a)

'r5' EGFR(b,l!_),Shc(a) -> EGFR(b!1,l!_),Shc(a!1)

'r6' EGFR(b!_) -> EGFR(b)

'r7' EGFR(b!1,l!_),Shc(a!1,b),Sos(x) -> EGFR(b!2,l!_),Shc(a!2,b!1),Sos(x!1)

'r8' Shc(b!_) -> Shc(b)

%init: EGFR(a,b,l),Sos(x),Shc(a,b),EGF(r)
